{
  "manifest_version": 2,
  "name": "MetaMask SDK Extension",
  "version": "0.1",
  "description": "Injects MetaMask SDK into every page",
  "homepage_url": "https://metamask.io/sdk",
  "author": "MetaMask",
  "samsungInternet": {
    "supported": true
  },
  "permissions": ["activeTab", "tabs", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["metamask-sdk.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
