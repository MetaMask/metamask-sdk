    "react": "^18.2.0",
    "react-native": "^0.71.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "lavamoat": {
    "allowScripts": {
      "eciesjs>secp256k1": false,
      "socket.io-client>engine.io-client>ws>bufferutil": false,
      "socket.io-client>engine.io-client>ws>utf-8-validate": false
    }
